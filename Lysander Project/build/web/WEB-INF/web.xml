<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>signpController</servlet-name>
        <servlet-class>controller.signpController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookInLysanderRatesController</servlet-name>
        <servlet-class>controller.bookInLysanderRatesController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookInLysanderRateController</servlet-name>
        <servlet-class>controller.bookInLysanderRateController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>bookInLysanderCardDetailsController</servlet-name>
        <servlet-class>controller.bookInLysanderCardDetailsController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookinData</servlet-name>
        <servlet-class>controller.bookInLysanderController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>signpController</servlet-name>
        <url-pattern>/signpController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookInLysanderRatesController</servlet-name>
        <url-pattern>/bookInLysanderRatesController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookInLysanderRateController</servlet-name>
        <url-pattern>/bookInLysanderRateController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>bookInLysanderCardDetailsController</servlet-name>
        <url-pattern>/bookInLysanderCardDetailsController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookinData</servlet-name>
        <url-pattern>/bookLysanderController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
